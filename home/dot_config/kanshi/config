output "eDP-1" {
  mode 1920x1200@60.00
  position 0,0
  scale 1
  alias $INTERNAL
}

output "Dell Inc. DELL P2725D 3S5S864" {
  mode 2560x1440@99.95Hz
  position 0,0
  scale 1
  alias $WORK_1
}

output "Dell Inc. DELL P2725D 24CRZ64" {
  mode 2560x1440@99.95Hz
  position 2560,0
  scale 1
  alias $WORK_2
}

profile docked {
  output $INTERNAL disable
  output $WORK_1 enable
  output $WORK_2 enable
}

profile fallback {
  output * enable
}
